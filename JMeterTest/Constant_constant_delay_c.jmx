          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Java PreProcessor Splits the Data File" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.commons.io.FileUtils
import java.io.File
import java.util.Random

// Define the path to the CSV file
String csvFilePath = &quot;{Data File Path}&quot;;

// Read the CSV file into a list of lines
List&lt;String&gt; lines = FileUtils.readLines(new File(csvFilePath), &quot;UTF-8&quot;)

// Random object to determine the number of rows for each thread
Random random = new Random()
int minRows = 50  // Minimum number of rows
int maxRows = 100  // Maximum number of rows

// Use a unique property name for each thread group
String counterPropertyName = &quot;VaryingPayloadTestCounter&quot; 

// Synchronized block to ensure thread safety
synchronized (this) {
    // Read and update the shared index
    int sharedIndex = Integer.parseInt(props.get(counterPropertyName))
    int numberOfRows = random.nextInt(maxRows - minRows + 1) + minRows

    // Building the payload
    StringBuilder payload = new StringBuilder()
    payload.append(lines.get(0)).append(&quot;\\n&quot;)
    for (int i = sharedIndex + 1; i &lt; sharedIndex + numberOfRows &amp;&amp; i &lt; lines.size(); i++) {
        payload.append(lines.get(i)).append(&quot;\\n&quot;)
    }

    // Update sharedIndex for the next thread
    props.put(counterPropertyName, String.valueOf(sharedIndex + numberOfRows))

    // Set the payload as a variable
    vars.put(&quot;payload&quot;, payload.toString())
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
